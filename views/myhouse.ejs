<!DOCTYPE html>
<html>

<head>
	<title>My Household</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
	<link rel="stylesheet" href="style.css">
	<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.4.8/socket.io.js"></script>
	<script>
		var socket = io.connect('http://localhost:3005');
		socket.on('connect', function (data) {
			console.log('connected');
			socket.emit('here', { my: 'data' });
		});
	</script>
</head>

		<body>
            <div class="site-wrapper">

			<header class="masthead-clearfix">
				<form class="logout" action="/logout" method="get">
					<input type="submit" name="logout" value="Log Out">
				</form>
				<form class="change" action="/households" method="get">
					<input type="submit" name="change" value="Change Household">
				</form>
				<form class="addhousehold" action="/addhousehold" method="get">
					<input type="submit" name="joinhousehold" value="Join Household">
				</form>
				<form class="newhousehold" action="/newhousehold" method="get">
					<input type="submit" name="newhousehold" value="Create New Household">
				</form>
			</header>
			<main class="container-fluid">
				<div class="recipe-row col-xs-8">
					<fieldset class="col-xs-12">
						<h1><%=household%></h1>


							<% groceries.forEach(function(gro){ %>
									<ul class="col-xs-12">
											<li>
													<h3><%= gro.name %></h3>
											</li>
									</ul>
							<% }); %>
					</fieldset>
				</div>
				<div class="recipe-button-row col-xs-4">
					<fieldset class="clear-buttons">
							<% groceries.forEach(function(gro){ %>
								<ul class="col-xs-12">
									<li>
											<form class="delete" action="/delete" method="post">
												<input type="submit" name="delete" value="Delete">
											</form>
									</li>
								</ul>
							<% }); %>
					</fieldset>
				</div>
			</main>
			<footer class="footer col-xs-12 col-sm-12 col-md-12 col-lg-12">
				<form class="add" action="/add" method="post">
					<input type="submit" name="add-item" value="Add">
				</form>
				<form class="delete-all" action="/delete" method="post">
					<input type="submit" name="delete-all" value="Delete All">
				</form>
			</footer>
        </div>
    </body>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
</html>
